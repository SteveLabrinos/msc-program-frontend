steve_lab/WebstormProjects/msc-program-frontend/src/reportWebVitals.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/containers/FrontBuilder/FrontBuilder.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/hoc/Layout/Layout.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/Navigation/Footer/Footer.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/Navigation/Footer/PrivacyPolicy/PrivacyPolicy.js",[],["84","85"],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/Navigation/SideDrawer/SideDrawer.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/Navigation/Toolbar/Toolbar.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/UI/Modal/Modal.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/Navigation/Footer/FooterContent/FooterContent.js",["86"],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/Navigation/NavigationItems/NavigationItems.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/UI/Backdrop/Backdrop.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/Logo/Logo.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/Navigation/SideDrawer/DrawerToggle/DrawerToggle.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/UI/Button/Button.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/Navigation/NavigationItems/NavigationItem/NavigationItem.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/Navigation/Toolbar/ToolbarInfo/ToolbarInfo.js",[],"/Users/steve_lab/WebstormProjects/msc-program-frontend/src/assets/statics/staticContent.js",["87"],"/**\n * @author Stavros Lamprinos [stalab at linuxmail.org] on 13/1/2021.\n */\n\nconst staticContent = [\n    {\n        header: `Εισαγωγή`,\n        content: `Το απόρρητο των επισκεπτών του ιστοτόπου μας, είναι πολύ σημαντικό για εμάς και είμαστε αφοσιωμένοι στην προστασία του. Η πολιτική μας εξηγεί πως θα χρησιμοποιήσουμε τις προσωπικές σας πληροφορίες.\n                  Η συναίνεση σας στη χρήση των cookies σε συμφωνία με τους όρους της πολιτικής μας κατά την πρώτη σας επίσκεψη στον ιστότοπο, μας επιτρέπει την χρήση cookies κάθε φορά που επισκέπτεστε τον ιστότοπο μας.`\n    },\n    {\n        header: `Εύσημα`,\n        content: `Αυτό το έγγραφο δημιουργήθηκε με την χρήση προτύπου από την SEQ Legal (seqlegal.com)`\n    },\n    {\n        header: `Συλλογή προσωπικών πληροφοριών`,\n        content: `Οι παρακάτω τύποι προσωπικών πληροφοριών μπορούν να συλλεγούν, να αποθηκευτούν και να χρησιμοποιηθούν:\n\n                Πληροφορίες σχετικές με τον υπολογιστή σας μεταξύ των οποίων η διεύθυνση IP σας, η γεωγραφική σας τοποθεσία ο τύπος και η έκδοση του περιηγητή σας και το λειτουργικό σας σύστημα.\n                Πληροφορίες σχετικά με τις επισκέψεις σας και τη χρήση του ιστοτόπου περιλαμβάνοντας την εξωτερική πηγή, τη διάρκεια της επίσκεψης, της προβολές σελίδων και τη διαδρομή πλοήγησης στον ιστότοπο;\n                Πληροφορίες όπως η διεύθυνση email σας, την οποία εισάγετε κατά την εγγραφή στον ιστότοπο.\n                Πληροφορίες που εισάγετε κατά τη δημιουργία του προφίλ σας στον ιστότοπο μας—για παράδειγμα, το όνομα σας, οι φωτογραφίες προφίλ, το φύλο, η ημερομηνία γέννησης, η προσωπική σας κατάσταση, τα ενδιαφέροντα και τα χόμπι σας, πληροφορίες σχετικά με την εκπαίδευση σας και πληροφορίες σχετικά με την απασχόληση σας.\n                Πληροφορίες, όπως το όνομα και το email σας, τις οποίες εισάγετε κατά την ρύθμιση των συνδρομών σας και για την αποστολή των emails και/η των ενημερωτικών δελτίων.\n                Πληροφορίες που εισάγετε κατά τη χρήση υπηρεσιών στον ιστότοπο μας.\n                Πληροφορίες που προκύπτουν κατά τη χρήση του ιστότοπου, όπως πότε, πόσο συχνά και κάτω από ποιες συνθήκες το χρησιμοποιείτε.\n                Πληροφορίες σχετικές με τις αγορές σας, τις υπηρεσίες που χρησιμοποιείτε και τις συναλλαγές που κάνετε στον ιστότοπο, οι οποίες περιλαμβάνουν το όνομα, τη διεύθυνση, τον τηλεφωνικά σας αριθμό, τη διεύθυνση του email σας και της πληροφορίες της πιστωτικής σας κάρτας.\n                Πληροφορίες που κοινοποιείτε στον ιστότοπο με την πρόθεση της δημοσίευσης στο διαδίκτυο, οι οποίες περιλαμβάνουν το όνομα χρήστη σας τις πληροφορίες προφίλ σας και το περιεχόμενο των δημοσιεύσεών σας.\n                Πληροφορίες που περιέχονται σε οποιαδήποτε επικοινωνία σας με εμάς μέσω email ή μέσω του ιστοτόπου, περιλαμβάνοντας το περιεχόμενο επικοινωνίας και τα μεταδεδομένα (metadata).\n                Οποιαδήποτε άλλη προσωπική πληροφορία μας στέλνετε.\n                Προτού αποκαλύψετε οποιαδήποτε προσωπικές πληροφορίες κάποιου τρίτου ατόμου, θα πρέπει να έχετε τη συγκατάθεση αυτού του ατόμου σχετικά `\n    },\n    {\n        header: `Διεθνείς μεταφορές δεδομένων`,\n        content: `Οι πληροφορίες που συλλέγουμε μπορεί να αποθηκευτούν να επεξεργαστούν και να μετακινηθούν μεταξύ οποιωνδήποτε χωρών στις οποίες λειτουργούμε έτσι ώστε να μπορούμε να χρησιμοποιήσουμε σύμφωνα με την παρούσα πολιτική.\n                Οι πληροφορίες που συλλέγουμε μπορούν να μεταφερθούν στις ακόλουθες χώρες που δεν διαθέτουν αντίστοιχη νομοθεσία προστασίας προσωπικών δεδομένων με αυτή που εφαρμόζεται στην Ευρωπαϊκή Οικονομική Περιοχή: οι ΗΠΑ, η Ρωσία η Ιαπωνία η Κίνα και η Ινδία.\n                Οι προσωπικές πληροφορίες που δημοσιεύετε στον ιστότοπο μας ή υποβάλλετε προς δημοσίευση στον ιστότοπο μας, δύνανται είναι διαθέσιμες μέσω διαδικτύου σε όλο τον κόσμο. Δεν μπορούμε να αποτρέψουμε τη χρήση ή τη λανθασμένη χρήση τέτοιων πληροφοριών από τρίτους.`\n    },\n    {\n        header: `Τρίτοι ιστότοποι`,\n        content: `Ο ιστότοπος μας περιλαμβάνει υπερσυνδέσμους σε, και πληροφορίες από, τρίτους ιστότοπους. Δεν μπορούμε να ελέγξουμε και δεν φέρουμε ευθύνη για τις πολιτικές προστασίας και της πρακτικές τρίτων μερών.`\n    },\n    {\n        header: `Ενημέρωση πληροφοριών`,\n        content: `Παρακαλώ ενημερώστε μας αν οι πληροφορίες που έχουμε για εσάς χρειάζονται ενημέρωση ή διόρθωση.`\n    },\n];\n\n\nconst terms = {\n    title: 'Όροι Χρήσης',\n    description: 'Εδώ θα αναλύονται οι όροι χρήσης που ισχύουν για την εφαρμογή'\n}\n\nconst announcements = [\n    {\n        title: 'Εγγραφές Χειμερινού Εξαμήνου 2021',\n        description: 'Σας γνωρίζουμε ότι αναρτήθηκε η συμπληρωματική  προκήρυξη των αιτήσεων για' +\n            ' να αιτήσεις φοιτητών για Σπουδές και πρακτική Άσκηση για το χειμερινό εξάμηνο του Ακ. Έτους 2020-2021.',\n        fullPost: 'Ανακοινώνεται στους επιτυχόντεςτου ΠΜΣ «ΠΛΗΡΟΦΟΡΙΑΚΑ ΣΥΣΤΗΜΑΤΑ» ότι οι εγγραφές ' +\n            'για το Β’ εξάμηνο ακαδημαϊκού έτους 2020-21 θα πραγματοποιηθούν με ηλεκτρονική υποβολήτων  δικαιολογητικών' +\n            '  που  απαιτούνται,  στο email της  Γραμματείας  του  Τμήματος, από Δευτέρα 18 έως και Παρασκευή 29' +\n            ' Ιανουαρίου 2021 με θέμα αποστολής : «ΕΓΓΡΑΦΕΣ Β’ ΕΞΑΜΗΝΟΥ»'\n    },\n    {\n        title: 'Ωρολόγιο Πρόγραμμα Διδασκαλίας, Έναρξη και Τρόπος Διεξαγωγής Μαθημάτων Β’ Εξαμήνου ακαδ. έτους 2020-21',\n        description: 'Σας ενημερώνουμε ότι η ημερομηνία έναρξης των μαθημάτων, στο ΠΜΣ “Πληροφοριακά Συστήματα”' +\n            ' για το Β’ εξάμηνο, είναι η Δευτέρα 1 Μαρτίου 2021.',\n        fullPost: 'Σας ενημερώνουμε ότι η ημερομηνία έναρξης των μαθημάτων, στο ΠΜΣ “Πληροφοριακά Συστήματα”' +\n            ' για το Β’ εξάμηνο, είναι η Δευτέρα 1 Μαρτίου 2021. Τα μαθήματα μέχρι νεωτέρας θα πραγματοποιούνται' +\n            ' εξ αποστάσεως.'\n    },\n    {\n        title: 'Πρόγραμμα εξεταστικής περιόδου Ιανουαρίου 2021',\n        description: 'Σας ενημερώνουμε ότι η ημερομηνία έναρξης της εξεταστικής περιόδου, στο ΠΜΣ “Πληροφοριακά ' +\n            'Συστήματα” για το Β’ εξάμηνο, είναι η Δευτέρα 23 Ιανουαρίου 2021.',\n        fullPost: 'Σας ενημερώνουμε ότι η ημερομηνία έναρξης της εξεταστικής περιόδου, στο ΠΜΣ “Πληροφοριακά ' +\n            'Συστήματα” για το Β’ εξάμηνο, είναι η Δευτέρα 23 Ιανουαρίου 2021. Τα μαθήματα μέχρι νεωτέρας' +\n            ' θα πραγματοποιούνται εξ αποστάσεως.'\n    },\n    {\n        title: 'Ανακοίνωση eclass',\n        description: 'Απανεργοποίηση μη ενεργών λογαριασμών στο eclass',\n        fullPost: 'Οι φοιτητές Α και Β κύκλων σπουδών που διαθέτουν παλαιό λογαριασμό στο σύστημα eclass.gr' +\n            ' με σύνδεση με credentials απευθείας στην πλατφόρμα (και όχι ιδρυματικό λογαριασμό) ενημερώνονται ' +\n            'ότι στις 3/6/2021 θα λήξει η ισχύς του λογαριασμού τους'\n    }\n]\n\nexport  { staticContent, terms, announcements };","/Users/steve_lab/WebstormProjects/msc-program-frontend/src/components/Navigation/Footer/TermsOfUse/TermsOfUse.js",[],{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","severity":1,"message":"93","line":2,"column":19,"nodeType":"94","messageId":"95","endLine":2,"endColumn":29},{"ruleId":"92","severity":1,"message":"96","line":54,"column":7,"nodeType":"94","messageId":"95","endLine":54,"endColumn":20},"no-native-reassign",["97"],"no-negated-in-lhs",["98"],"no-unused-vars","'FaFacebook' is defined but never used.","Identifier","unusedVar","'announcements' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]